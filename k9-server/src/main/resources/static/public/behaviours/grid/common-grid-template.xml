<svg:svg xmlns:svg='http://www.w3.org/2000/svg' xmlns='http://www.kite9.org/schema/adl' xmlns:xlink="http://www.w3.org/1999/xlink">

  <!-- tables -->
  
  <template id="rect" k9-indicator="path rect">
      <svg:rect x='0' y='0' width='#{$width}' height='#{$height}' rx="#{$template-1}" ry="#{$template-2}" />
  </template>
  
  <template id='table'>
    <back />
    <contents />
  </template>
  
  <template id="cell" k9-ui="drop insert layout copy invisible replace-cell" k9-palette="replace-cell">
    <back />
    <contents />
  </template>

</svg:svg>

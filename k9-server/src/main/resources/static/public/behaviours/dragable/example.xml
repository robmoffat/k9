<?xml version="1.0"?>
<svg:svg xmlns:svg='http://www.w3.org/2000/svg'>
  <svg:defs>
    <svg:style type="text/css"> 
       @import url("/public/examples/common/basic.css");
       @import url("dragable.css");
    </svg:style>
    
  </svg:defs>
  <diagram xmlns="http://www.kite9.org/schema/adl" drag="target">

     <g id="b1" drag="yes" class="box"><g id="fs1" class="text">A</g></g>
     
     <g class="group" drag="yes target" id="bigbox" style="kite9-layout: horizontal; ">
       <g id="b2" drag="yes" class="box"><g id="fs2" class="text">B</g></g>
       
       <g id="b3" drag="no" class="box"><g id="fs3" class="text">Can't Drag</g></g>
     </g>
     
     <g class="link" id="link2">
      <from drag="from" id="link2-from" reference="b1" class="arrow"></from>
      <to drag="to" id="link2-to" class="arrow" reference="b3"></to>
     </g>
     
     
     <g class="link" id="link2">
      <from drag="from" id="link2-from" reference="b1" class="hoverable"></from>
      <to drag="to" id="link2-to" class="hoverable" reference="b2"></to>
     </g>

  </diagram>
</svg:svg>
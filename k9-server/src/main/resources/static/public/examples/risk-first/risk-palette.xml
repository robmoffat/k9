<svg:svg xmlns:xlink='http://www.w3.org/1999/xlink' xmlns:svg='http://www.w3.org/2000/svg'>
  <svg:defs>
    <svg:style type="text/css"> 
      @import url("/public/examples/risk-first/risk-first.css"); 
    </svg:style>
  </svg:defs>
  <diagram xmlns="http://www.kite9.org/schema/adl" id="The Diagram" style="kite9-layout: down; kite9-padding: 0;">

    <table style="kite9-grid-size: 2 10; kite9-margin: 0; kite9-padding: 0">
      <cell  class="shaded">
        <risk class="coordination" id="coordination" />
      </cell>
      <cell >
        <risk class="feature" id="feature" style="kite9-horizontal-align: left; " />
      </cell>
      <cell  class="shaded">
        <risk class="map-and-territory" id="map-and-territory" />
      </cell>
      <cell class="shaded">
        <risk class="communication" id="communication" />
      </cell>
      <cell >
        <risk class="agency" id="agency" style="kite9-horizontal-align: left; " />
      </cell>
      <cell  class="shaded">
        <risk class="complexity" id="complexity" style="kite9-horizontal-align: left; " />
      </cell>
      <cell  class="shaded">
        <risk class="boundary" id="boundary" />
      </cell>
      <cell style=" kite9-layout: down; ">
        <risk class="dependency" id="dependency" />
        <risk class="operational" id="operational" style="kite9-horizontal-align: left; " />
      </cell> 

      <cell style="kite9-occupies: 1 1 0 1; kite9-layout: right; ">
        <container style="kite9-layout: down; kite9-sizing: maximize;">
          <group style="kite9-layout: right; ">
            <risk class="feature-fit" id="feature-fit" />
            <risk class="implementation" id="implementation" />
          </group>
          <risk class="conceptual-integrity" id="conceptual-integrity" />
          <label>
            <text>Fit</text>
            <symbols />
          </label>
        </container>

        <container style="kite9-layout: down; kite9-sizing: maximize; ">
          <risk class="market" id="market" />
          <risk class="feature-access" id="feature-access"/>

          <label>
            <text>Audience</text>
            <symbols />
          </label>
        </container>


        <container style="kite9-layout: down; kite9-sizing: maximize; ">
          <risk class="regression" id="regression"/>
          <risk class="feature-drift" id="feature-drift"/>

          <label>
            <text>Evolution</text>
            <symbols />
          </label>
        </container>
      </cell>


      <cell style="kite9-occupies: 1 1 2 3; " class="shaded">
        <group style="kite9-layout: right; ">
          <container layout="down">
            <risk class="internal-model" id="internal-model"/>
            <risk class="learning-curve" id="learning-curve" />
            <label>
              <text>Internal Model</text>
              <symbols />
            </label>
          </container>

          <container layout="down" style="kite9-sizing: maximize; ">
            <risk class="message" id="message" />
            <risk class="invisibility" id="invisibility" />
            <label>
              <text>Message</text>
              <symbols />
            </label>
          </container>
          
          <container layout="down" style="kite9-sizing: maximize; ">
            <risk class="protocol" id="protocol" />
            <label>
              <text>Protocol</text>
              <symbols />
            </label>
          </container>
          <container layout="down" style="kite9-sizing: maximize; ">

            <risk class="channel" id="channel" />

            <label>
              <text>Channel</text>
              <symbols />
            </label>

          </container>
        </group>

      </cell>

      <cell style="kite9-occupies: 1 1 4 4" >
        <risk class="trust" id="trust"/>
        <risk class="security" id="security"/>
      </cell>

      <cell style="kite9-occupies: 1 1 5 5; kite9-layout: right; " class="shaded">
        <risk class="codebase" id="codebase" />
        <risk class="dead-end" id="dead-end" />
      </cell>

      <cell style="kite9-occupies: 1 1 6 7; kite9-layout: right; ">
        <container layout="right" style="kite9-sizing: maximize; ">
          <group layout="down">
            <risk class="scarcity" id="scarcity" />
            <risk class="funding" id="funding"/>
            <risk class="staff" id="staff"/>
          </group>
          <group layout="down">
            <risk class="opportunity" id="opportunity"/>
            <risk class="schedule" id="schedule"/>
               <risk class="red-queen" id="red-queen"/>
          </group>
          <label>
            <text>Scarcity</text>
            <symbols />
          </label>
        </container>

        <group layout="down">
          <risk class="deadline" id-="deadline" />
          <risk class="reliability" id="reliability"/>
        </group>
        <group layout="down">
          <risk class="software-dependency" id="software-dependency"/>
          <risk class="process" id="process" />
        </group>
      </cell>

    </table>
  </diagram>

</svg:svg>
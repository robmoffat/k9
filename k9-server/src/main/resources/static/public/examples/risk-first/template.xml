	    <svg:ellipse rx='20' ry='20' cy='20' cx='20' stroke-width="1px" style='fill: url(#risk-background); '  class="glyph-back"/>
    </back>
  </template>
  
  <template id='glyph-rect' k9-ui="drag drop delete insert align connect layout autoconnect">

      <svg:rect x='0' y='0' width='#{$width}' height='#{$height}' style='fill: url(#risk-background); ' class="glyph-back" />
    </back>
    <svg:g class="glyph">
      <text style="kite9-type: text;">d</text>
    </svg:g>

<?xml version="1.0"?>
<svg:svg xmlns:svg='http://www.w3.org/2000/svg'>
  <svg:defs>
    <svg:style type="text/css"> 
       @import url("/public/examples/common/basic.css");
       @import url("/public/context/editor/editor.css");
    </svg:style>
    
  </svg:defs>
  <diagram xmlns="http://www.kite9.org/schema/adl" id="dia">

     <g id="b1" class="box"><g id="fs1" class="text">b1</g></g>
     
     <g class="group" id="bigbox" style="kite9-layout: horizontal; ">
       <g id="b2" class="box"><g id="fs2" class="text">b2</g></g>
       
       <g id="b3" class="box"><g id="fs3" class="text">b3</g></g>
     </g>
     
     <g class="link" id="link1" r="r">
      <from id="link1-from" reference="b1" class="arrow"></from>
      <to id="link1-to" class="arrow" reference="b3"></to>
     </g>
     
     
     <g class="link" id="link2">
      <from id="link2-from" reference="b1" class="hoverable selectable"></from>
      <to id="link2-to" class="hoverable selectable" reference="b2"></to>
     </g>

  </diagram>
</svg:svg>